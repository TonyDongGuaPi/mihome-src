    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/mishopsdk_orange_btn_left_cur"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/mishopsdk_orange_btn_left_cur"/>
