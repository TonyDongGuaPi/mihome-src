<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:dist="http://schemas.android.com/apk/distribution" xmlns:PtrHeader="http://schemas.android.com/apk/PtrHeader" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:miui="http://schemas.android.com/apk/res/miui" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/device_more_frame" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:orientation="vertical" android:id="@+id/device_more" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content">
        <FrameLayout android:id="@+id/title_bar" android:background="@color/white" android:layout_width="match_parent" android:layout_height="@dimen/titlebar_height">
            <TextView android:textSize="@dimen/font_size_2" android:textColor="@color/title_bar_text_color" android:ellipsize="end" android:layout_gravity="center" android:id="@+id/module_a_3_return_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/titlebar_return_area_size" android:layout_marginRight="@dimen/titlebar_return_area_size" android:maxLines="1" android:layout_marginStart="@dimen/titlebar_return_area_size" android:layout_marginEnd="@dimen/titlebar_return_area_size"/>
            <TextView android:textSize="@dimen/title_bar_text_size" android:textColor="@color/title_bar_text_color" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/module_a_3_return_more_more_btn" android:background="@drawable/device_more_close" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="17dp" android:scaleType="center" android:layout_marginEnd="17dp"/>
            <View android:layout_gravity="bottom" android:background="@color/list_bg_line" android:layout_width="match_parent" android:layout_height="1px"/>
        </FrameLayout>
        <RelativeLayout android:id="@+id/body22" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/title_bar">
            <LinearLayout android:orientation="vertical" android:id="@+id/menu_container" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        </RelativeLayout>
        <View android:id="@+id/empty" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/body22"/>
    </RelativeLayout>
</RelativeLayout>
