        <TextView android:id="@+id/face_circle_reset" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Code:"/>
        <TextView android:id="@+id/smile_machine_code" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0"/>
    </LinearLayout>
