    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:layout_width="120dp" android:layout_height="wrap_content" android:text="rightEyeOcclusion:"/>
