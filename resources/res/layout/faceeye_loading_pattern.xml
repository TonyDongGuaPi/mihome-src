<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:dist="http://schemas.android.com/apk/distribution" xmlns:PtrHeader="http://schemas.android.com/apk/PtrHeader" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:customPullDownRefreshLL="http://schemas.android.com/apk/res/com.xiaomi.smarthome" xmlns:miui="http://schemas.android.com/apk/res/miui" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/face_eye_loading_page" android:background="#88000000" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <ImageView android:layout_gravity="center" android:id="@+id/simple_face_preview" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
    </FrameLayout>
    <RelativeLayout android:id="@+id/zoloz_container" android:layout_width="@dimen/zoloz_container_width" android:layout_height="@dimen/zoloz_container_height" android:layout_marginTop="@dimen/zoloz_container_margin_top" android:layout_centerHorizontal="true">
        <com.alipay.zoloz.toyger.widget.UploadProgressBar android:id="@+id/zoloz_back_progress" android:layout_width="@dimen/zoloz_back_progress_width" android:layout_height="@dimen/zoloz_back_progress_height" android:layout_centerInParent="true" app:eye_end_angle="345" app:eye_max="3600" app:eye_round_color="#e2e2e2" app:eye_round_progress_color="#108ee9" app:eye_round_width="5dp" app:eye_start_angle="25" app:eye_text_color="#9a32cd" app:eye_text_is_displayable="false" app:eye_text_size="18sp"/>
        <ImageView android:layout_width="68dp" android:layout_height="25dp" android:src="@drawable/zoloz_logo" android:layout_centerInParent="true"/>
    </RelativeLayout>
    <TextView android:textSize="20dp" android:textColor="#919191" android:id="@+id/simple_process_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:text="@string/face_eye_processing" android:layout_below="@+id/zoloz_container" android:layout_centerHorizontal="true"/>
</RelativeLayout>
